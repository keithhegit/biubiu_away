{"remainingRequest":"D:\\Code\\anhei\\VueJS-Diablo3-main\\node_modules\\babel-loader\\lib\\index.js!D:\\Code\\anhei\\VueJS-Diablo3-main\\src\\directives\\Index.js","dependencies":[{"path":"D:\\Code\\anhei\\VueJS-Diablo3-main\\src\\directives\\Index.js","mtime":1737563993350},{"path":"D:\\Code\\anhei\\VueJS-Diablo3-main\\babel.config.js","mtime":1737563991985},{"path":"D:\\Code\\anhei\\VueJS-Diablo3-main\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1737596043366},{"path":"D:\\Code\\anhei\\VueJS-Diablo3-main\\node_modules\\babel-loader\\lib\\index.js","mtime":1737596055671}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"names":["Vue","directive","inserted","el","focus","bind","binding","color","bone","white","style","fontFamily","console","log","arg","Object","keys","modifiers","length","value","borderExp","bottom","borderBottom","paddingBottom","top","borderTop","paddingTop"],"sources":["D:/Code/anhei/VueJS-Diablo3-main/src/directives/Index.js"],"sourcesContent":["import Vue from 'vue'\n\nVue.directive('focus', {\n  /**\n     * inserted hook: Cuando el elemento es insertado en el DOM\n     * @param el {HTMLElement} El elemento al que está dirigida la directiva\n     */\n  inserted: function (el) {\n    // Enfoca el elemento\n    el.focus()\n  }\n})\n\nVue.directive('diablo', {\n  /**\n     * @param el {HTMLElement} Elemento al que aplica la directiva\n     * @param binding {DirectiveBinding} Datos que recibe la directiva a traves de argumentos, modificadores, etc\n     */\n  bind (el, binding) {\n    // Definimos los colores\n    const color = {\n      bone: '#e8dcc2',\n      white: '#ffffff'\n    }\n\n    // Tipografía Diablo\n    el.style.fontFamily = 'DiabloHeavy, sans-serif'\n\n    console.log('binding')\n    console.log(binding)\n\n    // Argumento (`:`)\n    const arg = binding.arg === 'bone' ? 'bone' : 'white'\n    // Si el argumento es 'bone'\n    if (arg === 'bone') {\n      // Le damos el color blanco hueso\n      el.style.color = color.bone\n    } else {\n      // Si no, el color por defecto es el blanco\n      el.style.color = color.white\n    }\n\n    // Modificadores (`.`)\n    // Si hay modificadores\n    if (Object.keys(binding.modifiers).length > 0) {\n      // Value: (`=`)\n      // Valor por defecto de 'value' es 1 (Corresponde al ancho del borde en 'px')\n      const value = binding.value || 1\n      // Expresión con el borde, ejemplo: '1px solid white'\n      const borderExp = `${value}px solid ${color[arg]}`\n\n      // Si el modificador es 'bottom'\n      if (binding.modifiers.bottom) {\n        // Ponemos una linea debajo del elemento\n        el.style.borderBottom = borderExp\n        // Separada 10px\n        el.style.paddingBottom = '10px'\n      }\n      // Si el modificador es 'top'\n      if (binding.modifiers.top) {\n        // Ponemos una linea encima del elemento\n        el.style.borderTop = borderExp\n        // Separada 10px\n        el.style.paddingTop = '10px'\n      }\n    }\n  }\n})\n"],"mappings":"AAAA,OAAOA,GAAG,MAAM,KAAK;AAErBA,GAAG,CAACC,SAAS,CAAC,OAAO,EAAE;EACrB;AACF;AACA;AACA;EACEC,QAAQ,EAAE,SAAAA,CAAUC,EAAE,EAAE;IACtB;IACAA,EAAE,CAACC,KAAK,CAAC,CAAC;EACZ;AACF,CAAC,CAAC;AAEFJ,GAAG,CAACC,SAAS,CAAC,QAAQ,EAAE;EACtB;AACF;AACA;AACA;EACEI,IAAIA,CAAEF,EAAE,EAAEG,OAAO,EAAE;IACjB;IACA,MAAMC,KAAK,GAAG;MACZC,IAAI,EAAE,SAAS;MACfC,KAAK,EAAE;IACT,CAAC;;IAED;IACAN,EAAE,CAACO,KAAK,CAACC,UAAU,GAAG,yBAAyB;IAE/CC,OAAO,CAACC,GAAG,CAAC,SAAS,CAAC;IACtBD,OAAO,CAACC,GAAG,CAACP,OAAO,CAAC;;IAEpB;IACA,MAAMQ,GAAG,GAAGR,OAAO,CAACQ,GAAG,KAAK,MAAM,GAAG,MAAM,GAAG,OAAO;IACrD;IACA,IAAIA,GAAG,KAAK,MAAM,EAAE;MAClB;MACAX,EAAE,CAACO,KAAK,CAACH,KAAK,GAAGA,KAAK,CAACC,IAAI;IAC7B,CAAC,MAAM;MACL;MACAL,EAAE,CAACO,KAAK,CAACH,KAAK,GAAGA,KAAK,CAACE,KAAK;IAC9B;;IAEA;IACA;IACA,IAAIM,MAAM,CAACC,IAAI,CAACV,OAAO,CAACW,SAAS,CAAC,CAACC,MAAM,GAAG,CAAC,EAAE;MAC7C;MACA;MACA,MAAMC,KAAK,GAAGb,OAAO,CAACa,KAAK,IAAI,CAAC;MAChC;MACA,MAAMC,SAAS,GAAG,GAAGD,KAAK,YAAYZ,KAAK,CAACO,GAAG,CAAC,EAAE;;MAElD;MAC